    "require": {
        "php": ">=5.6.0",
        "zodream/zodream" : "dev-master",
        "zodream/disk" : "dev-master",
        "zodream/helpers" : "dev-master",
        "zodream/http" : "dev-master",
        "zodream/route" : "dev-master",
        "zodream/template" : "dev-master",
